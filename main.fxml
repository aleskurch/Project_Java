<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.company.LoginController">
   <children>
      <TextField fx:id="login" layoutX="220.0" layoutY="113.0" promptText="login" />
      <Button fx:id="signIn" layoutX="257.0" layoutY="313.0" mnemonicParsing="false" onAction="#onSignInClick" prefHeight="27.0" prefWidth="87.0" text="Sign In" textAlignment="CENTER" textFill="#b71ebc">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <PasswordField fx:id="password" layoutX="220.0" layoutY="173.0" promptText="password" />
      <Button fx:id="signUp" layoutX="484.0" layoutY="39.0" mnemonicParsing="false" onAction="#onSignUpClick" text="Sign Up" textFill="#910a94" />
      <Text fill="#c61caf" layoutX="221.0" layoutY="78.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Log in TV">
         <font>
            <Font size="40.0" />
         </font>
      </Text>
      <Label fx:id="label" layoutX="170.0" layoutY="212.0" prefHeight="32.0" prefWidth="261.0" textFill="RED" />
      <Label fx:id="Lable2" layoutX="190.0" layoutY="262.0" prefHeight="27.0" prefWidth="229.0" textFill="#05ff22" />
   </children>
</AnchorPane>
